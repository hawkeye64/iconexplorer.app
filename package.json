{
  "name": "iconexplorer.app",
  "version": "3.0.2",
  "description": "Icon Explorer application for Quasar Framework SVG icons. 140k+ searchable icons and growing.",
  "productName": "Icon Explorer for Quasar Framework SVG Icons",
  "author": "Jeff Galbraith <jeff@quasar.dev>",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.5.1",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint -c ./eslint.config.js \"./src*/**/*.{ts,js,cjs,mjs,vue}\"",
    "format": "prettier --write \"**/*.{js,ts,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "test": "echo \"No test specified\" && exit 0",
    "dev": "quasar dev",
    "build": "quasar build",
    "postinstall": "quasar prepare"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.17",
    "pinia": "^3.0.1",
    "quasar": "^2.17.7",
    "quasar-extras-svg-icons": "^2.1.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@quasar/app-vite": "^2.1.1",
    "@quasar/quasar-app-extension-qmarkdown": "^2.0.4",
    "@types/node": "^22.13.5",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.4.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.2",
    "typescript": "~5.7.3",
    "vite-plugin-checker": "^0.9.0",
    "vue-tsc": "^2.2.4"
  },
  "engines": {
    "node": "^28 || ^26 || ^24 || ^22 || ^20 || ^18",
    "npm": ">= 6.13.4",
    "yarn": ">= 1.21.1"
  }
}